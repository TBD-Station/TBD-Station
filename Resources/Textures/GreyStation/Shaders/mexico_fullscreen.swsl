uniform sampler2D SCREEN_TEXTURE;

void fragment() {
    highp vec4 color = zTextureSpec(SCREEN_TEXTURE, UV);
	float r = color.r * 0.73926 + color.g * -0.07573 + color.b * -0.08072;
	float g = color.r * 1.16571 + color.g * -0.19147 + color.b * -0.01675;
	float b = color.r * -0.2949903 + color.g * -0.777115 + color.b * 0.4100697;
    COLOR = vec4(r, g, b, color.a);
}
